<?xml version="1.0" encoding="utf-8"?><!-- Android P 限制了明文流量的网络请求，非加密的流量请求都会被系统禁止掉 -->
<network-security-config>
    <base-config cleartextTrafficPermitted="true">
        <trust-anchors>
            <certificates
                src="system"
                overridePins="true" />
            <certificates
                src="user"
                overridePins="true" />
        </trust-anchors>
    </base-config>
</network-security-config>